                android:layout_height="35dp"
                android:textSize="16sp"
                android:layout_marginTop="10sp"
                android:layout_toRightOf="@id/takeOutImageView1"
                android:textStyle="bold"
                android:text="111111"/>

            <TextView
                android:id="@+id/takeOutProductAddress1"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:textSize="16sp"
                android:layout_below="@id/takeOutProductName1"
                android:layout_alignLeft="@id/takeOutProductName1"
                android:text="222222"
                android:textColor="@color/black" />

            <ImageButton
                android:id="@+id/takeOut_checked1"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_toRightOf="@+id/takeOutImageView1"
                android:layout_marginLeft="130dp"
                android:background="@drawable/round_button"
                android:layout_centerVertical="true"
                android:scaleType="centerCrop"
                app:srcCompat="@drawable/unchecked" />


        </RelativeLayout>
